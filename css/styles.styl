h1 {
  text-align center;
  font-weight normal;
  margin 0;
  font-size: 2rem;
}

h2,h3,h4,h5,h6 {
  font-family: 'robotomedium';
  font-weight normal;
  font-size: 1.6rem;
  text-transform: uppercase;
}

h2 {
  text-align center;
  margin-top 0;
  margin-bottom 4rem;
  font-size: 3rem;
}






