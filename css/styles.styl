// CSS Normalize
// Import all partials
@require './_*.styl'

html {
  font-size: 10px;
}

body {
  margin: 0;
  padding: 0;
  font-size: 1.6rem;
  font-family: 'robotoregular', sans-serif;
  font-weight normal;
  color: #262626;
}

.inner {
  margin: 0 auto;
  max-width: 100%;
}

.our-prices {
    max-width: 100%;
    padding: 4% 1%;
    color: #999;
    background: #000;
    h2 {
      color: #fff;
    }
}

